div
            .form-group.row
                label.col-2(for="referer") Referer: 
                input.col-9.form-control(type="text" name="referer", id="referer")
            .form-group.row
                label.col-2(for="proxy") Proxy: 
                input.col-9.form-control(type="text" name="proxy", id="proxy", placeholder="ip:port")
            //.form-group.row
                label.col-2(for="cilck") Click:
                input.col-9.form-control(type="text" name="click", id="click", placeholder="")
            .form-inline
                label.col-2.mr-2(for="timeout") Timeout(s): 
                input.col-2.mr-2.form-control(type="number" name="timeout", id="timeout", max="300", min="30", step="30", value="60")
                label.col-2.mr-2(for="delay") Delay(s): 
                input.col-2.mr-2.form-control(type="number" name="delay", id="delay", max="60", min="0", step="5", value="10")
            br

            .form-group
                label(for="actions") Actions: 
                textarea.form-control(id="actions", name="actions", rows=2)
            .form-group
                label(for="exHeaders") Extra Headers: 
                textarea.form-control(id="exHeaders", name="exHeaders", rows=2)
            .form-inline
              .form-check-inline
                    label.mr-2.form-check-label(for="disableScript")= "disable script: "
                    input.mr-2.form-check-input(type="checkbox", name="disableScript", id="disableScript" value="")
                    label.mr-2.form-check-label(for="dom")= " wait until dom: "
                    input.mr-2.form-check-input(type="checkbox", name="dom", id="dom" value="")
                    label.mr-2.form-check-label(for="bulksave")= " bulk save: "
                    input.mr-2.form-check-input(type="checkbox", name="bulksave", id="bulksave" value="" checked)
                    label.mr-2.form-check-label(for="afp")= " anti fingerprint: "
                    input.mr-2.form-check-input(type="checkbox", name="afp", id="afp" value="")

            //label(for="postData") Post Data: 
            //br
            //textarea(id="postData", name="postData", rows=3, cols=50)
            //br
            input(type="hidden" name="_csrf", value=csrfToken)
            input(type="submit" name="submit", value="submit")
